# -*- Mode: Bash; tab-width: 4; indent-tabs-mode: nil; -*-
#
#       Setup the paths of the homebrew packaging system.

if test -x "$(command -v brew)"; then
    if test -f "$(brew --prefix)/etc/bash_completion" ; then
        . $(brew --prefix)/etc/bash_completion
    fi
fi
